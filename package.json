{
  "name": "rename-cli",
  "version": "7.0.0",
  "description": "A command line utility for renaming files",
  "main": "rename.js",
  "preferGlobal": true,
  "bin": {
    "rename": "./bin.js",
    "rname": "./bin.js"
  },
  "scripts": {
    "test": "mocha --file tests/test.js",
    "build": "pkg --out-path bin -t latest-win-x64,latest-macos-x64,latest-linux-x64 ."
  },
  "author": "Jordan Hotmann",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jhotmann/node-rename-cli.git"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "cli-clear": "^1.0.4",
    "clipboardy": "^2.2.0",
    "fs-extra": "^8.1.0",
    "globby": "^11.0.0",
    "inquirer": "^7.1.0",
    "jpeg-exif": "^1.1.4",
    "moment": "^2.25.0",
    "num2fraction": "^1.2.2",
    "nunjucks": "^3.2.1",
    "opn": "^5.3.0",
    "path-exists": "^3.0.0",
    "prompt-sync": "^4.1.6",
    "terminal-kit": "^1.35.2",
    "traverse": "^0.6.6",
    "yargs": "^15.3.0"
  },
  "pkg": {
    "assets": [
      "lib/userData.js",
      "lib/userFilters.js",
      "node_modules/terminal-kit/**/*"
    ]
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-mocha": "^6.3.0",
    "mocha": "^7.1.2"
  }
}
